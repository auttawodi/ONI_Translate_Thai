msgctxt "STRINGS.CREATURES.SPECIES.FLUTEGG.NAME"
msgid "Pacu Egg"
msgstr "วางไข่"

msgctxt "STRINGS.CREATURES.SPECIES.GEYSER.DESC"
msgid "A highly pressurized geyser that periodically erupts, spraying <style=\"gas\">Steam</style> and boiling hot <style=\"liquid\">Water</style>."
msgstr ""

msgctxt "STRINGS.CREATURES.SPECIES.GEYSER.NAME"
msgid "Steam Geyser"
msgstr "ปล่องไอน้ำร้อน"

msgctxt "STRINGS.CREATURES.SPECIES.GLOM.DESC"
msgid ""
"An agile, <style=\"disease\">Disease</style>-ridden invertebrate.\n"
"\n"
"It is attracted to the decomposing carcasses of deceased organisms and frequently emits small bursts of <style=\"gas\">Polluted Oxygen</style>."
msgstr ""

#, fuzzy
msgctxt "STRINGS.CREATURES.SPECIES.GLOM.NAME"
msgid "Morb"
msgstr "การถูพื้นสามารถทำได้ดี"

#, fuzzy
msgctxt "STRINGS.CREATURES.SPECIES.GREEDYGREEN.DESC"
msgid "A rapidly growing, subterranean <style=\"plant\">Plant</style>."
msgstr "อุณหภูมิที่เกิดปฏิกิริยา <style=\"plant\">Plant</style>."

msgctxt "STRINGS.CREATURES.SPECIES.GREEDYGREEN.NAME"
msgid "Avari Vine"
msgstr ""

msgctxt "STRINGS.CREATURES.SPECIES.GRONEHOG.DESC"
msgid ""
"A skittish mammalian creature.\n"
"\n"
"It moves in herds for safety."
msgstr ""

msgctxt "STRINGS.CREATURES.SPECIES.GRONEHOG.NAME"
msgid "Volgus"
msgstr ""

msgctxt "STRINGS.CREATURES.SPECIES.GRONEHOGMOUND.DESC"
msgid "The communal nesting ground of a Volgus herd."
msgstr ""

msgctxt "STRINGS.CREATURES.SPECIES.GRONEHOGMOUND.NAME"
msgid "Volgus Den"
msgstr ""

msgctxt "STRINGS.CREATURES.SPECIES.HATCH.DESC"
msgid ""
"A nocturnal scavenger that will consume anything within reach that fits into its large mouth.\n"
"\n"
"The waste excretions of a Hatch can be used as <style=\"solid\">Fuel</style>.\n"
"\n"
